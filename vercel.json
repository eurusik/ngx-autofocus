{
  "framework": "angular",
  "buildCommand": "npm run build:demo",
  "outputDirectory": "dist/demo",
  "installCommand": "npm install",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)\\.(js|css|ico|json)", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
